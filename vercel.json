{
  "version": 2,
  "builds": [
    {
      "src": "/api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/",
      "dest": "/api/index"
    },
    {
      "src": "/admin",
      "dest": "/api/admin"
    },
    {
      "src": "/(.*)/editor",
      "dest": "/api/redirect_editor"
    },
    {
      "src": "/upload",
      "dest": "/api/upload.js"
    },
    {
      "src": "/blog/(.*)",
      "dest": "/api/blog.js"
    }
  ]
}